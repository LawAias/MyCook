<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <include layout="@layout/toolbar_login"
    android:id="@+id/toolbar"/>


    <ImageView
        android:layout_below="@+id/toolbar"
        android:src="@drawable/xiafancai"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/xiafancai"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/xiafancai"
        android:textSize="17sp"
        android:id="@+id/textView1"
        android:layout_below="@+id/xiafancai"
        android:layout_alignLeft="@+id/xiafancai"
        android:layout_alignStart="@+id/xiafancai"/>

    <ImageView
        android:src="@drawable/jiacahngcai"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/jiachangcai"
        android:layout_alignTop="@+id/biandang"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp"/>


    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/jiachangcai"
        android:textSize="17sp"
        android:id="@+id/textView3"
        android:layout_below="@+id/jiachangcai"
        android:layout_alignLeft="@+id/jiachangcai"
        android:layout_alignStart="@+id/jiachangcai"/>
    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/biandang"
        android:textSize="17sp"
        android:id="@+id/textView2"
        android:layout_below="@+id/biandang"
        android:layout_alignLeft="@+id/biandang"
        android:layout_alignStart="@+id/biandang"/>

    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/sushi"
        android:textSize="17sp"
        android:id="@+id/textView4"
        android:layout_below="@+id/sushi"
        android:layout_alignLeft="@+id/sushi"
        android:layout_alignStart="@+id/sushi"/>

    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/tianpin"
        android:textSize="17sp"
        android:id="@+id/textView5"
        android:layout_below="@+id/tianpin"
        android:layout_alignLeft="@+id/tianpin"
        android:layout_alignStart="@+id/tianpin"/>

    <ImageView
        android:src="@drawable/lanrenshipu"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/lanrenshipu"
        android:layout_marginTop="28dp"
        android:layout_below="@+id/textView4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"/>


    <ImageView
        android:src="@drawable/tangpin"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/tangpin"
        android:layout_alignTop="@+id/lanrenshipu"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"/>
    <ImageView
        android:src="@drawable/mianshi"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/mianshi"
        android:layout_alignTop="@+id/tangpin"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"/>

    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/tangpin"
        android:textSize="17sp"
        android:id="@+id/textView8"
        android:layout_below="@+id/tangpin"
        android:layout_alignLeft="@+id/tangpin"
        android:layout_alignStart="@+id/tangpin"/>

    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/mianshi"
        android:textSize="17sp"
        android:id="@+id/textView9"
        android:layout_below="@+id/mianshi"
        android:layout_alignLeft="@+id/mianshi"
        android:layout_alignStart="@+id/mianshi"/>

    <ImageView
        android:src="@drawable/biandang"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/biandang"
        android:layout_alignTop="@+id/xiafancai"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/shala"
        android:textSize="17sp"
        android:id="@+id/textView6"
        android:layout_below="@+id/shala"
        android:layout_alignLeft="@+id/shala"
        android:layout_alignStart="@+id/shala"/>

    <TextView
        android:layout_width="100dip"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:text="@string/lanrenshipu"
        android:textSize="17sp"
        android:id="@+id/textView7"
        android:layout_below="@+id/lanrenshipu"
        android:layout_alignLeft="@+id/lanrenshipu"
        android:layout_alignStart="@+id/lanrenshipu"/>

    <ImageView
        android:src="@drawable/sushi"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/sushi"
        android:layout_alignTop="@+id/tianpin"
        android:layout_alignLeft="@+id/textView1"
        android:layout_alignStart="@+id/textView1"/>

    <ImageView
        android:src="@drawable/shala"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/shala"
        android:layout_alignTop="@+id/tianpin"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"/>

    <ImageView
        android:src="@drawable/tianpin"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:scaleType="centerCrop"
        android:id="@+id/tianpin"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"/>

</RelativeLayout>